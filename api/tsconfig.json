{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "declaration": false,
        "noEmit": true
    },
    "include": [
        "./**/*.ts"
    ],
    "exclude": [
        "_server.ts"
    ]
}