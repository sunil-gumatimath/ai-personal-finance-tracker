{
    "buildCommand": "bun run build",
    "installCommand": "bun install",
    "framework": null,
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/((?!api).*)",
            "destination": "/index.html"
        }
    ]
}